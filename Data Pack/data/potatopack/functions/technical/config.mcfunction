##################################################################################################
# WARNING: Be sure to use the Seed Conversion Tool so the Slime Chunk Check can function properly!
scoreboard players set seedHigh potatopack.math 0
scoreboard players set seedLow potatopack.math 0
##################################################################################################

# See the welcome message on load (0: False, 1: True, Default: 1)
scoreboard players set configSeeWelcomeMessage potatopack.math 1

# See warnings for some features (0: False, 1: True, Default: 1)
scoreboard players set configSeeWarnings potatopack.math 1

# See data pack names in item tooltips (0: False, 1: True, Default: 1)
scoreboard players set configSeePackName potatopack.math 1

# Give armour stands arms (0: False, 1: True, Default: 1)
scoreboard players set configArmorStandArms potatopack.math 1

# Toggle what the depth meter displays in its tooltip (0: Distance from sea level, 1: Y position, Default: 0)
scoreboard players set configToggleDepthMeter potatopack.math 0

# See position X and Z while looking at a map (0: False, 1: True, Default: 0)
scoreboard players set configSeeMapCoords potatopack.math 0

# Get a free map when you first join the world (0: False, 1: True, Default: 1)
scoreboard players set configGiveStarterMap potatopack.math 1